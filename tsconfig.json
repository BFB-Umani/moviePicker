{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "moviepicker/components/*": ["src/components/*"],
      "moviepicker/reduxStore/*": ["./src/store/*"],
      "moviepicker/screens/*": ["src/screens/*"],
      "moviepicker/constants/*": ["src/constants/*"],
      "moviepicker/navigation/*": ["src/navigation/*"],
      "moviepicker/firestore/*": ["src/firestore/*"],
      "moviepicker/assets/*": ["assets/*"],
      "moviepicker/styles/*": ["src/styles/*"],
      "moviepicker/src/*": ["src/*"],
      "moviepicker/root/*": ["*"],
    },
  },
  "files:": ["./custom.d.ts"]
}
